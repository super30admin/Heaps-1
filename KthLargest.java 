class Solution 
{
    public int findKthLargest(int[] nums, int k) 
    {
        PriorityQueue<Integer> queue = new PriorityQueue<>((a,b)->b-a);
        
        for(int i: nums)
        {
            queue.add(i);
        }
        
        int count = 1;
        
        while(count < k)
        {
            queue.poll();
            count++;
        }
        
        return queue.poll();
    }
}